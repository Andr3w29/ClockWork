
@{

    ViewBag.Title = "Service Logs";
}



<div class="col-xs-12">
    <div class="box">
        <div class="box-header">
            <h3 class="box-title">
                Request and Response Logs
            </h3>
        </div>
        <div class="box-body">

            <div id="service-log-list">
                <table class="table table-hover table-striped">
                    <thead>
                        <tr>
                            <th>Work Station</th>
                            <th>Domain</th>
                            <th>LogType</th>
                            <th>Request/Response</th>
                            <th>createdOn</th>
                            <th>&nbsp;</th>
                        </tr>
                    </thead>
                    <tbody id="service-log"></tbody>
                </table>
            </div>
        </div>

    </div>
</div>
@Html.Partial("_modalForm")
@section pagescripts {
    <script src="~/Content/js/app/modalform.js"></script>
    <script src="~/Content/js/app/servicelog/cw.servicelogs.js"></script>

    <script type="text/javascript">
        $(function () {
            ServiceLog.init();

        });
    </script>

}